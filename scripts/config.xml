<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!--Used for restore the file which is source code-->
    <targetpath>
        <windowspath>D:\Temp\target</windowspath>
        <linuxpath>usr1/temp/target</linuxpath>
    </targetpath>
    <!--Used for restore the file which is original file-->
    <sourcepath>
        <windowspath>D:\Temp\source</windowspath>
        <linuxpath>usr1/temp/source</linuxpath>
    </sourcepath>

    <!--The repository of git setting-->
    <gits username="wybuguin@hotmail.com" passwd="12345">
        <!--The config for git. There can be mutil repository of git-->
        <git id="1" username="wybuguin@hotmail.com" passwd="12345">
            <!--This config is used for download code form git-->
            <gitpath>
                https://github.com/Buguin/learngit.git
            </gitpath>
            <!--If foldername is empty, code will download to the folder which name the suffix of gitpath-->
            <foldername>
            </foldername>
        </git>
        <git id="2" username="wybuguin@hotmail.com" passwd="12345">
            <gitpath>
                https://github.com/Buguin/game-of-life.git
            </gitpath>
            <foldername>
                game
            </foldername>
        </git>
    </gits>
    <!--The repository of svn setting-->
    <svns username="wybuguin@hotmail.com" password="12345">
        <svn username="wybuguin@hotmail.com" password="12345">
            <!--This config is used for download code form git-->
            <svnpath>
                https://github.com/Buguin/learngit.git
            </svnpath>
            <!--If foldername is empty, code will download to the folder which name the suffix of gitpath-->
            <foldername>
            </foldername>
        </svn>
    </svns>
</config>